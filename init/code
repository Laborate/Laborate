NameVirtualHost *:80

<VirtualHost *:80>
    ServerAdmin webmaster@localhost
    ServerName code.dev.laborate.io
    DocumentRoot /var/www/code/public_html/views
    ErrorLog /var/www/code/server/apache/error.log
    LogLevel debug
    CustomLog /var/www/code/server/apache/access.log combined
</VirtualHost>


<VirtualHost *:80>
    ServerAdmin webmaster@localhost
    ServerName resources.code.dev.laborate.io
    DocumentRoot /var/www/code/public_html/resources
    ErrorLog /var/www/code/server/apache/error_resources.log
    LogLevel debug
    CustomLog /var/www/code/server/apache/access_resources.log combined
</VirtualHost>